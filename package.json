{
  "name": "nima-bot",
  "version": "0.0.1",
  "description": "A little Discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env npm run clean && babel -s -d dist src",
    "clean": "rimraf dist",
    "dev": "babel-node --trace-warnings ./src/index.js",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "start": "node --trace-warnings ./dist/index.js"
  },
  "author": "Deniz Dogan <denizdogan@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "discord.js": "^11.3.0",
    "dotenv-safe": "^4.0.4",
    "fs-extra": "^5.0.0",
    "mathjs": "^3.20.1",
    "moment": "^2.20.1",
    "money": "^0.2.0",
    "node-fetch": "^1.7.3",
    "parse-int": "^1.0.2",
    "query-string": "^5.0.1",
    "random-index": "^1.0.4",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/node": "^7.0.0-beta.38",
    "@babel/polyfill": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2"
  }
}
